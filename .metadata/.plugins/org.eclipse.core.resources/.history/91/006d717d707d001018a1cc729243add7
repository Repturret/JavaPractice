package sec02.exam07;

public class FloatDoubleExample {
	
	public static void main(String[] args) {
//		float var1 = 3.14;
		float var2 = 3.14f;
		double var3 = 3.14;
		
		float var4 = 0.1234567890123456789f;
		double var5 = 0.1234567890123456789;
		
		System.out.println("var2: " + var2);
		
	}

}
